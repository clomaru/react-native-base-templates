{
	"name": "reactNativeBaseTemplate",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"prestorybook": "rnstl",
		"storybook": "storybook start -p 7007"
	},
	"dependencies": {
		"@types/storybook__addon-actions": "^3.0.3",
		"@types/storybook__addon-links": "^3.3.0",
		"@types/storybook__react": "^3.0.7",
		"react": "16.3.1",
		"react-native": "0.55.4"
	},
	"config": {
		"react-native-storybook-loader": {
			"searchDir": ["./lib"],
			"pattern": "**/*.stories.js",
			"outputFile": "./storybook/storyLoader.js"
		}
	},
	"devDependencies": {
		"@storybook/addon-actions": "^3.4.5",
		"@storybook/addon-links": "^3.4.5",
		"@storybook/addons": "^3.4.5",
		"@storybook/react-native": "^3.4.5",
		"@types/jest": "^22.2.3",
		"@types/react": "^16.3.14",
		"@types/react-native": "^0.55.15",
		"@types/react-test-renderer": "^16.0.1",
		"babel-core": "^6.26.3",
		"babel-jest": "22.4.4",
		"babel-preset-react-native": "4.0.0",
		"babel-runtime": "^6.26.0",
		"jest": "22.4.4",
		"prettier": "^1.13.4",
		"prop-types": "^15.6.1",
		"react-dom": "16.3.1",
		"react-test-renderer": "16.3.1",
		"styled-components": "^3.2.6",
		"styled-components-ts": "0.0.14",
		"ts-jest": "^22.4.6",
		"tslint": "^5.10.0",
		"tslint-plugin-prettier": "^1.3.0",
		"typescript": "^2.8.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": ["ts", "tsx", "js"],
		"transform": {
			"^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
			"\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"testPathIgnorePatterns": [
			"\\.snap$",
			"<rootDir>/node_modules/",
			"<rootDir>/lib/"
		],
		"cacheDirectory": ".jest/cache"
	}
}
